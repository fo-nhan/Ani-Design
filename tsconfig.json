{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "outDir": "./dist",
      "moduleResolution": "node",
      "declarationMap": true
  
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }